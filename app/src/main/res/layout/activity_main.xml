<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/textView"
        android:layout_width="195dp"
        android:layout_height="64dp"
        android:text="Presenting..."
        android:textSize="24sp"
        android:textStyle="italic"
        app:fontFamily="serif-monospace"
        tools:layout_editor_absoluteX="104dp"
        tools:layout_editor_absoluteY="101dp" />

    <Button
        android:id="@+id/button"
        android:layout_width="277dp"
        android:layout_height="61dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:background="#0000FF"
        android:text="Administrator"
        android:textColor="#FFFFFF "
        android:textSize="14sp"
        android:typeface="sans"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="502dp" />

    <Button
        android:id="@+id/button2"
        android:layout_width="187dp"
        android:layout_height="48dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:background="#0000FF"
        android:text="Voter"
        android:textColor="#FFFFFF "
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="605dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="291dp"
        android:layout_height="53dp"
        android:text="Log in with required document as...."
        android:textColor="@color/colorPrimary"
        android:textSize="15sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="86dp"
        tools:layout_editor_absoluteY="449dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="294dp"
        android:layout_height="85dp"
        android:text="With advanced Geo - technology"
        android:textColor="@color/colorPrimary"
        android:textSize="16sp"
        android:textStyle="bold"
        app:fontFamily="casual"
        tools:layout_editor_absoluteX="117dp"
        tools:layout_editor_absoluteY="378dp" />

    <Button
        android:id="@+id/button3"
        android:layout_width="415dp"
        android:layout_height="51dp"
        android:background="#F332C3"
        android:text="Microsoft    Code.fun.do"
        android:textColor="#F8F5F6"
        android:textSize="16sp"
        android:textStyle="bold"
        android:typeface="monospace"
        tools:layout_editor_absoluteX="-2dp"
        tools:layout_editor_absoluteY="-5dp" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/Logo"
        tools:layout_editor_absoluteX="86dp"
        tools:layout_editor_absoluteY="212dp" />

</androidx.constraintlayout.widget.ConstraintLayout>